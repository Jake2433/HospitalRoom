<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#002060">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="온도 / 습도 : "
        android:textColor="#ffffff"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="0 ºC / 0 F"
        android:textColor="#ffffff"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView2"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <Button
        android:id="@+id/Btn_FirstP"
        android:layout_width="180dp"
        android:layout_height="48dp"
        android:background="#00000000"
        android:onClick="CliFirstP"
        android:text="EMPTY"
        android:textColor="#ffffff"
        app:layout_constraintTop_toBottomOf="@+id/Iv_FirstP"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/Btn_SixthP"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="16dp" />

    <Button
        android:id="@+id/Btn_SecondP"
        android:layout_width="180dp"
        android:layout_height="48dp"
        android:background="#00000000"
        android:onClick="CliSecondP"
        android:text="EMPTY"
        android:textColor="#ffffff"
        app:layout_constraintTop_toBottomOf="@+id/Iv_SecondP"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/Btn_FifthP"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="16dp" />

    <Button
        android:id="@+id/Btn_ThirdP"
        android:layout_width="180dp"
        android:layout_height="48dp"
        android:layout_marginRight="8dp"
        android:background="#00000000"
        android:onClick="CliThirdP"
        android:text="EMPTY"
        android:textColor="#ffffff"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Iv_ThirdP"
        android:layout_marginEnd="8dp"
        app:layout_constraintLeft_toRightOf="@+id/Btn_FourthP"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp" />

    <Button
        android:id="@+id/Btn_FourthP"
        android:layout_width="180dp"
        android:layout_height="48dp"
        android:layout_marginLeft="26dp"
        android:background="#00000000"
        android:onClick="CliFourthP"
        android:text="EMPTY"
        android:textColor="#ffffff"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Iv_FourthP"
        android:layout_marginStart="26dp" />

    <Button
        android:id="@+id/Btn_FifthP"
        android:layout_width="180dp"
        android:layout_height="48dp"
        android:background="#00000000"
        android:onClick="CliFifthP"
        android:text="EMPTY"
        android:textColor="#ffffff"
        app:layout_constraintTop_toBottomOf="@+id/Iv_FifthP"
        android:layout_marginLeft="26dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="26dp" />

    <Button
        android:id="@+id/Btn_SixthP"
        android:layout_width="180dp"
        android:layout_height="48dp"
        android:background="#00000000"
        android:onClick="CliSixthP"
        android:text="EMPTY"
        android:textColor="#ffffff"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Iv_SixthP"
        android:layout_marginLeft="26dp"
        android:layout_marginStart="26dp" />

    <android.support.v7.widget.AppCompatImageButton
        android:id="@+id/Btn_add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="#00000000"
        android:onClick="CliAdd"
        android:text="입실"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Btn_SixthP"
        app:layout_constraintVertical_bias="0.652"
        app:srcCompat="@drawable/checkin"
        android:layout_marginBottom="8dp" />

    <android.support.v7.widget.AppCompatImageButton
        android:id="@+id/Btn_rmv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:background="#00000000"
        android:onClick="CliRmv"
        android:text="퇴실"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Btn_FirstP"
        app:layout_constraintVertical_bias="0.652"
        app:srcCompat="@drawable/checkout"
        app:layout_constraintLeft_toRightOf="@+id/Btn_add"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="1.0" />

    <android.support.v7.widget.AppCompatImageView
        android:id="@+id/Iv_FirstP"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/patient1"
        android:onClick="CliFirstP"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/Btn_SecondP"
        app:layout_constraintLeft_toRightOf="@+id/Iv_SixthP"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="8dp"
        app:layout_constraintHorizontal_bias="0.65" />

    <android.support.v7.widget.AppCompatImageView
        android:id="@+id/Iv_SecondP"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/patient2"
        android:onClick="CliSecondP"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/Btn_ThirdP"
        app:layout_constraintLeft_toRightOf="@+id/Iv_FifthP"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="8dp"
        app:layout_constraintHorizontal_bias="0.65" />

    <android.support.v7.widget.AppCompatImageView
        android:id="@+id/Iv_ThirdP"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/patient3"
        android:onClick="CliThirdP"
        app:layout_constraintLeft_toRightOf="@+id/Iv_FourthP"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="8dp"
        app:layout_constraintHorizontal_bias="0.65"
        android:layout_marginTop="72dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <android.support.v7.widget.AppCompatImageView
        android:id="@+id/Iv_SixthP"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/patient6"
        android:onClick="CliSixthP"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/Btn_FifthP"
        android:layout_marginLeft="60dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="60dp" />

    <android.support.v7.widget.AppCompatImageView
        android:id="@+id/Iv_FifthP"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/patient5"
        android:onClick="CliFifthP"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/Btn_FourthP"
        android:layout_marginLeft="60dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="60dp" />

    <android.support.v7.widget.AppCompatImageView
        android:id="@+id/Iv_FourthP"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/patient4"
        android:onClick="CliFourthP"
        android:layout_marginLeft="60dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="72dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        android:layout_marginStart="60dp" />

</android.support.constraint.ConstraintLayout>